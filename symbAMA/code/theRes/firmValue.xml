<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AMAModel modelName="firmValue">
    <endogenousVariable name="vv"/>
    <endogenousVariable name="dd"/>
    <parameter name="delta" defaultValue=".3"/>
    <parameter name="rr" defaultValue=".2"/>
    <parameter name="lrd" defaultValue="1."/>
    <equation>((1+rr)*vv[t])-((vv[t+1]+dd[t+1]))</equation>
    <equation>(dd[t]-lrd)-(((1-delta)*(dd[t-1]-lrd)))</equation>
</AMAModel>
