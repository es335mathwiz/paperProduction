@misc {STMAZ.04516291,
    author       = {Homble, P.},
    title        = {Ergodicity conditions for nonlinear discrete time stochastic dynamical systems with Markovian noise.},
    howpublished = {Stoch. Anal. Appl. 11, 513-568 (1993).},
    year         = {1993},
    abstract     = {Summary: Let $\{\xi_n;n\ge 0\}$ be an ergodic Markovian noise process with invariant measure $\pi_\xi$. Sufficient conditions are given for the ergodicity of the pair Markov process $\{(x_n,\xi_n); n\ge 0\}$ arising from the nonlinear stochastic difference equation $x_{n+1}=f(x_n,\xi_n)$ on a manifold $M$ and with $f$ being at least a continuous map. An essential condition for ergodicity is the existence of a unique maximal invariant control set $C$ for the associated control system $x_{n+1}=f(x_n,u_n),u_n\in$ supp $(\pi_\xi)$, the support of $\pi_\xi$. It is shown that under some further hypotheses involving compactness and weak stochastic controllability, the set $C\times\text{ supp }(\pi_\xi)$ is Harris recurrent for the pair process $\{(x_n,\xi_n); n\ge 0\}$ and that the invariant measure on $C\times\text{supp}(\pi_\xi)$ is unique and finite. Since geometric control theory is used to prove ergodicity, several results concerning control sets of discrete time dynamical systems are also given.},
    identifier   = {S353.03165},
}

