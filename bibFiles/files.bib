@article{nerlove04,
	author={Marc Nerlove},
	year=2004,
	month={Special Issue},
	title={Programming Languages: A Short History for Economists},
	journal={Journal of economic and social measurement},
	volume=29,
	number={1-3},
	pages={189-203},
	note={UD: 200410},
	abstract={The development of programming languages as a means of communicating with digital computers is surveyed with emphasis on those languages and characteristics of particular relevance to economists and statisticians. There are several different types of software: (1) true programming languages which are used for many purposes in communicating with the machine, such as FORTRAN; (2) software with greater specificity in purpose and function such as MATHEMATICA, GAUSS, MATLAB and MAPLE; (3) "libraries" or collections of special purpose algorithms designed to be used within programs written in one of the major languages, e.g., the LINPACK, MINPACK, EISPAC, and IMSL libraries; (4) Collections of programs, sometimes called packages, linked together and designed for special purposes--these generally must be used within some of the languages one step removed from the basics. Examples include MATLAB Toolboxes; and (5) finally, there are programs which although made generally available cannot be used stand-alone in any sense.},
	keywords={History of Economic Thought: Econometrics; Quantitative Studies (B230)Computational Techniques (C630)Econometric Software (C870); Software},
